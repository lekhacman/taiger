{
  "name": "taiger",
  "version": "1.0.0",
  "author": "andrew <khacmanle@gmail.com>",
  "scripts": {
    "start": "parcel src/public/index.html",
    "pretest": "eslint .",
    "test": "jest",
    "fix": "eslint --fix src"
  },
  "files": [
    "src"
  ],
  "devDependencies": {
    "@types/angular": "^1.6.56",
    "@types/angular-ui-bootstrap": "^1.0.0",
    "@types/angular-ui-router": "^1.1.40",
    "@types/jest": "^24.0.18",
    "@types/ramda": "^0.26.26",
    "coveralls": "^3.0.5",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-xo": "^0.26.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^3.0.1",
    "jest": "^24.8.0",
    "lint-staged": "^9.2.0",
    "parcel": "^1.12.3",
    "prettier": "^1.18.2"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "repository": "git@github.com:lekhacman/taiger.git",
  "license": "UNLICENSED",
  "private": true,
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true,
      "browser": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "trailingComma": "es5",
          "singleQuote": true
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "angular": "^1.7.8",
    "angular-sanitize": "^1.7.8",
    "angular-ui-bootstrap": "^2.5.6",
    "angular-ui-router": "^1.0.23",
    "bootstrap": "^4.3.1",
    "ramda": "^0.26.1",
    "templates": "^1.2.9"
  }
}
