{
  "name": "taiger",
  "version": "1.0.0",
  "author": "andrew <khacmanle@gmail.com>",
  "homepage": "http://lekhacman-taiger.s3-website-ap-southeast-1.amazonaws.com",
  "scripts": {
    "start": "parcel src/public/index.html",
    "pretest": "eslint src",
    "test": "jest --coverage",
    "fix": "eslint --fix src",
    "build": "parcel build src/public/index.html"
  },
  "files": [
    "src"
  ],
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-transform-modules-commonjs": "^7.6.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@types/angular": "^1.6.56",
    "@types/angular-ui-bootstrap": "^1.0.0",
    "@types/angular-ui-router": "^1.1.40",
    "@types/jest": "^24.0.18",
    "@types/ramda": "^0.26.26",
    "coveralls": "^3.0.5",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-xo": "^0.26.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^3.0.1",
    "jest": "^24.8.0",
    "lint-staged": "^9.2.0",
    "parcel": "^1.12.3",
    "prettier": "^1.18.2"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "repository": "git@github.com:lekhacman/taiger.git",
  "license": "UNLICENSED",
  "private": true,
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true,
      "browser": true
    },
    "rules": {
      "new-cap": "off",
      "capitalized-comments": "off",
      "prettier/prettier": [
        "error",
        {
          "trailingComma": "es5",
          "singleQuote": true
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-modules-commonjs",
      "@babel/plugin-transform-runtime"
    ]
  },
  "dependencies": {
    "angular": "^1.8.0",
    "angular-sanitize": "^1.7.8",
    "angular-ui-bootstrap": "^2.5.6",
    "angular-ui-router": "^1.0.23",
    "bootstrap": "^4.3.1",
    "ramda": "^0.26.1"
  }
}
